.dialog{position:fixed;width:100%;height:100%;background:rgba(0,0,0,0.2);z-index:9;top:0;left:0;}
.dialog-con{width:14.93rem;height:7.33rem;background:#fff;border-radius:0.13rem;position:absolute;left:50%;top:50%;margin:-4.98rem 0 0 -7.46rem;}
.dialog-text{font-size:0.85rem;padding-top:1.28rem;color:#4c4948;line-height:1.28rem; text-align:center;}
.dialog-bank{font-size:0.85rem;padding-top:0.53rem;color:#4c4948;line-height:1.28rem; text-align:center;}
.dialog-link{display:block;width:100%;height:2.32rem;border-top:2px solid #08af66;line-height:2.32rem;position:absolute;bottom:0;left:0;font-size:0.96rem;color:#08af66;text-align:center;}
.dialog-link b{padding-left:0.48rem;}
.dialog-con .close{background: url('../../images/NEW/rz_07.png') no-repeat center;width: 0.8rem;height: 0.8rem;background-size:0.8rem 0.8rem; position: absolute;top: -0.16rem;right: -0.16rem;}

.dialog-confirm{width:100%;height:2.32rem;line-height:2.32rem;position:absolute;bottom:0;left:0;font-size:0.96rem;color:#fff;text-align:center; background:#f64349;border-bottom-left-radius: 0.13rem;border-bottom-right-radius: 0.13rem;}

.dialog-confirm a{display:block;height:2.32rem;float:left; color:#fff; width:100%;}
.dialog-confirm a.all{ width:100%!important; border:none!important;}
.dialog-confirm a:nth-child(1){border-right:1px solid #fff;width:7.42rem;}
.dialog-confirm a:nth-child(2){width:7.42rem;}

.dialog-confirm a:nth-child(3){width:100%;}

