p.btn{ width:18.4rem; height:2.4rem; margin:1rem auto; border-radius:0.2rem; overflow:hidden;}
p.btn input[type=button]{ width:100%; height:100%; border:none; font-size:0.9rem; color:#fff; background:url(../../images/NEW/butt_star.png);}
.contpad{ padding-bottom:1.5rem;}

