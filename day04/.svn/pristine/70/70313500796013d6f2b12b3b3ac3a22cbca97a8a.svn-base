@charset "utf-8";
/* CSS Document */
body{font-size:14px; color:#333;}
body,div,p,h1,h2,h3,h4,ul,li,ol,span,i,em,dl,dt,dd,input,form,section,header,footer,button{ margin:0; padding:0;}
a{ color:#333; text-decoration:none;}
li{ list-style-type:none;}
input,select,textarea{ outline:none; -webkit-appearance:none;}
.fl{ float:left;}
.fr{ float:right;}
.both{ clear:both;}
.hide{ display:none;}
.fam_yh{ font-family:"微软雅黑", Arial, "宋体";}
.fam_ar{ font-family:Arial, "宋体", "微软雅黑"}
.f5{ background: #f5f5f5;}
.fff{ background:#fff !important;}
.f64349{ color:#f64349 !important;}
.ff6600{ color:#ff6600;}
.co999{ color:#999;}
.colorf00{ color:#f00;}
.bg_f64349{ background:url(../../images/NEW/f64349.jpg) center; background-size:100% 100%;}
.btopnone{ border-top:none !important;}
.bbomnone{ border-bottom:none !important;}
.brtnone{ border-right:none !important;}
.bnone{ border:none !important;}
.modal_bg{ width:100%; height:100%; position:absolute; top:0; left:0; background:#000; opacity:0.5; filter:alpha(opacity=50); z-index:8;}
.sub_success{ position:absolute; top:8rem; left:30%; width:40%; height:auto; text-align:center; font-size:0.8rem; color:#fff; overflow:hidden; padding:1rem 0; border-radius:0.3rem; background:#333; z-index:999;}
