label { display:inline;}
.regular-checkbox { display:none;}
.regular-checkbox + label { display:inline-block; position:relative;}
.regular-checkbox:checked + label { background:url(../images/quan_choice.png) right bottom no-repeat; background-size:40px 40px; box-shadow:0 1px 2px rgba(0,0,0,0.05), inset 0px -15px 10px -12px rgba(0,0,0,0.05), inset 15px 10px -12px rgba(255,255,255,0.1);}
.regular-checkbox:checked + label:after { content:''; font-size:14px; position:absolute; top:0px; left:3px; color:#99a1a7;}
.big-checkbox + label { width:100%; height:100%;}


