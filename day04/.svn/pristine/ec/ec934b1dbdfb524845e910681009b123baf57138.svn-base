.pop {position: absolute;top:6.87rem;left:50%;margin-left:-5.87rem;width:11.74rem ;height:5.6rem; font-size:0.85rem;line-height:2.82rem; border-radius: 0.42rem;text-align: center;background: #313131;color: white; }
.pop .pop_icon{width: 100%;height: 2.67rem;background-image:url(../../../images/NEW/error_icon.png);background-size:1.81rem 1.81rem;background-repeat:no-repeat;
background-position: center bottom;}

.modal01 .pop_onesure{width:13.33rem; height:2rem; line-height:2rem; font-size:0.96rem; color:#fff; background:#f64349; border-radius:1rem; margin:0.8rem auto 1.6rem;text-align: center; cursor:pointer;}
.modal01 .cont_img{text-align:center;padding-top:0.8rem;}
/*代扣协议*/
.header {position:relative;width: 100%;height: 2.4rem;line-height:2.6rem;text-align: center;font-size:0.96rem;background:#f64349;color: white; }
.header .back_to{position:absolute;top:0.36rem;left:0.8rem;display: inline-block;font-style: normal;width:1.33rem;height:1.33rem;}
.detail{padding: 0 0.8rem;}
.detail .title{height:4.02rem;line-height: 4.53rem;text-align:center;font-weight:normal;font-size:1.01rem;color:#f64349;}
.detail p{text-indent: 2em;padding-top:0.32rem;line-height:1.33rem;font-size: 0.64rem;}
.detail p.tipsInfor{line-height: 1.06rem;color: #141414;}
.detail p.paraFirst{color: #f64349;padding-top: 1.6rem;}
.detail p.subpara{padding-top: 0.86rem;line-height: 1.12rem;text-indent: 1.4em;}
.detail p.minpara{padding-top: 0.69rem;line-height: 0.96rem;text-indent: 1em;}