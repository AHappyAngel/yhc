 @charset "utf-8";
/* CSS Document */
@font-face{
font-family: myHyqihei;
src:url('HYQiHei-80S.otf');
}
@font-face{
font-family: mySimplifiedt;
src:url("FashionblackSimplified.ttf");
}
@font-face{
font-family: myHSansGBW;
src:url("HiraginoSansGBW3.otf");
}
.HSansGBW{font-family: myHSansGBW;}
.addBHeader {position:relative;height:2.4rem;line-height:2.44rem;background:#f64349;color: white; font-size: 0.96rem;text-align: center; }
.addBHeader .to_back{position:absolute;top:0.58rem;left:0.8rem;font-style: normal;display: inline-block;width:1.28rem;height:1.28rem;background-image: url(../../../images/NEW/back_icon.png);background-size: 100% 100%; }
.banktt{height: 2.30rem;line-height: 2.34rem;font-size:0.8rem;}
.add_bank{font-size: 0.8533rem;}
.add_bank ul li b{width:5.7rem;}
.add_bank ul li input{width:13.06rem;}
/*.add_bank ul li input.phone_num{width: 8.4rem;}*/
.add_bank ul li .getCode{position:absolute;top:0rem;left:13.8rem;width: 4.37rem;height:1.66rem;line-height:1.86rem;margin-top:0.42rem;border:0.026rem solid #f64349;border-radius:0.16rem;text-align:center;color: #f64349;font-family: myHSansGBW;}
.add_bank ul li .waitCode{position:absolute;top:0rem;left:13.8rem;width: 4.37rem;height:1.66rem;line-height:1.86rem;margin-top:0.42rem;font-size:0.53rem;color:#ccc;border:0.026rem solid #ccc;border-radius:0.16rem;text-align:center;font-family: myHSansGBW;}
.add_bank ul li select{width:4.9rem;}
.add_bank .bank{width:4.9rem; }
/*.add_bank ul li b.province{ background:url(../../images/KaNiu/bili03.png) center no-repeat; background-size:60% auto;}*/
.add_bank .bankNumber{color: #ccc;}

.tipCredit {height: 1.50rem;line-height: 1.50rem;}
.tipCredit span{font-size:0.5867rem;color: #666;}
.tipCredit a{color: #5491e1;}
.bank_bom p.tips span{line-height: 1.0rem;}
.showcard .showDomain{position:relative;width:100%;height:10.88rem;}
.showcard .cardbg{position:absolute;left:50%;top:0.5rem;margin-left:-7.97rem;width:15.94rem;height:9.92rem;background-image: url(../../../images/NEW/back_cardbg.png);background-size: 15.94rem 9.92rem;}
.showcard .cardName{padding:1.65rem 1.55rem 0rem;height:0.75rem;font-size: 0.8rem;color: white;font-family: myHyqihei;}
.showcard .bankType{display: inline-block;float: left;font-weight: normal;}
.showcard .cardType{display: inline-block;float: right;}
.showcard .cardNum{margin:0 auto;padding-top:2.51rem;font-size:0.96rem;font-family:mySimplifiedt;color: white;text-align:center;text-shadow:0.06rem 0.06rem 0.03rem #999;}
.showcard .cardNum .numSpace{padding-right:1.0rem;font-weight: normal;}
.showcard .cardNum .num1{padding-right: 0.07rem;}
.showcard .showInfor{padding-left: 1.23rem;background: white;width: 18.77rem;height: auto;font-size: 0.8533rem;overflow: hidden;}
.showInfor ul li { width:100%; height: 2.56rem;line-height: 2.58rem;border-bottom: 0.016rem #ddd solid;overflow: hidden;}
.showInfor ul li b{float: left;display: block;font-weight: 100;padding-left:0.59rem;width:5.7rem;overflow: hidden;}
.showInfor ul li input{width: 13.06rem;}
.showInfor ul li span{float: right;display: block;overflow: hidden;padding-right: 1.87rem;}
.bank_bom p.tips{font-size: 0.1rem;}


.modal01 .new_style{padding:1.54rem 0.96rem 1.33rem;line-height: 1.3333rem;color: #333;font-size: 0.8533rem;}
.modal01 .tipTimes{font-size:0.6933rem;color: #666;}
.modal01 .timesCredit{font-style: normal;color:#f64349;}

